{
  "name": "frontend",
  "version": "1.0.4",
  "private": true,
  "dependencies": {
    "chart.js": "4.3.0",
    "js-cookie": "3.0.5",
    "react": "18.2.0",
    "react-chartjs-2": "5.0.1",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "web-vitals": "3.3.2"
  },
  "devDependencies": {
    "@craco/craco": "7.1.0",
    "@craco/types": "7.1.0",
    "@types/js-cookie": "3.0.3",
    "@types/node": "20.3.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "eslint-config-google": "0.14.0",
    "react-scripts": "5.0.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "craco  start",
    "build": "craco  build",
    "test": "craco  test",
    "eject": "react-scripts eject",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix src/"
  },
  "eslintConfig": {
    "extends": [
      "google",
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "linebreak-style": "off",
      "max-len": [
        "warn",
        {
          "code": 160,
          "comments": 320
        }
      ],
      "space-in-parens": [
        "warn",
        "never"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "require-jsdoc": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
